    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.50",
    "@types/gapi.client": "^1.0.1",
    "@types/gapi.client.drive": "^3.0.3",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
